<library>
  <action id="reorder_primer_set_groups" target="primer_set_groups" type="update" href="ajax/primer_set_groups/tuple" scope="self" />
  <select id="primer_set_groups_transition">
    <option value="0">
      nothing at all
    </option>
  </select>
</library>
<panel id="primer_set_groups_panel" title="Primer Set Groups" open="false">
  <collection id="primer_set_groups" onreorder="reorder_primer_set_groups" select="single" extent="2">
    <schema id="primer_set_groups_schema">
      <elt type="string">Name</elt>
      <elt type="timestamp">Group Members</elt>
      <elt type="timestamp">Created</elt>
      <elt type="string">Owner</elt>
    </schema>
    <tuple>
      <elt>Example</elt>
      <elt>Primer Set 1, Primer Set 2</elt>
      <elt>Yesterday</elt>
      <elt>browndav</elt>
    </tuple>
    <tuple>
      <elt>Another Example</elt>
      <elt>Primer Set 1, Primer Set 2</elt>
      <elt>Yesterday</elt>
      <elt>browndav</elt>
    </tuple>
    <tuple>
      <elt>Third Example</elt>
      <elt>Primer Set 1, Primer Set 2</elt>
      <elt>Two Days Ago</elt>
      <elt>browndav</elt>
    </tuple>
  </collection>
  <panels id="primer_set_groups_detail" size="small">
    <panel title="Selection Details" open="true">
      <collection id="primer_set_groups_detail" type="form" appearance="normal">
        <tuple>
          <elt>
            <import id="primer_set_groups[primr_grp_nm]" ref="test_input" label="Primer Name" value="" />
          </elt>
        </tuple>
      </collection>
    </panel>
  </panels>
  <transition>
    <import ref="before_transition" />
    <import ref="primer_set_groups_transition" key="transition" selected="0" />
    <import ref="after_transition" />
  </transition>
</panel>

